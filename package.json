{
  "name": "kanban-playground",
  "description": "Qwik implementation of TodoMVC: https://todomvc.com/. Vite.js tooling.",
  "scripts": {
    "build": "npm run typecheck && npm run build.client && npm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.ssr.tsx",
    "dev": "vite",
    "dev.ssr": "node --inspect node_modules/vite/bin/vite.js --mode ssr",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js --mode ssr",
    "start": "npm run dev",
    "lint": "eslint \"src/**/*.ts*\"",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.0.34",
    "@types/eslint": "8.4.2",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.16.0",
    "eslint-plugin-qwik": "0.0.34",
    "node-fetch": "2.6.7",
    "typescript": "4.7.2",
    "vite": "2.9.9"
  },
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true,
  "dependencies": {
    "@ngneat/falso": "^6.0.2"
  }
}
